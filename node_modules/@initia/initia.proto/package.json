{
  "name": "@initia/initia.proto",
  "version": "0.1.33",
  "description": "JS and TS types relating to Protocol Buffers used by Initia project",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/initia-labs/initia.proto"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "amino/",
    "celestia/",
    "cosmos/",
    "cosmos_proto/",
    "cosmwasm/",
    "gogoproto/",
    "google/",
    "ibc/",
    "initia/",
    "minievm/",
    "miniwasm/",
    "noble/",
    "sdk/",
    "slinky/",
    "tendermint/",
    "*.md"
  ],
  "scripts": {
    "format": "prettier --write --loglevel warn \"./src/**/*.ts\"",
    "build": "rm -rf ./build && tsc --module commonjs && ./scripts/prepare.sh",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.6",
    "@types/long": "^5",
    "@types/node": "^20",
    "prettier": "^2.7.1",
    "ts-proto": "^1.166.2",
    "typescript": "^5"
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.15.0",
    "google-protobuf": "^3.21.0",
    "long": "^5.2.3",
    "protobufjs": "^7.1.1"
  }
}
